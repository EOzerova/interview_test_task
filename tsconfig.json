{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": false,
        "declaration": false,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "dist",
        "typeRoots": [ "./node_modules/@types" ],
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}